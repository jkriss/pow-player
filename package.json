{
  "name": "pow-player",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js --dir public --serve _/bundle.js --live -- -o public/_/bundle.js",
    "build:index": "browserify -p tinyify -t [ babelify --presets [ \"@babel/preset-env\" ] ] index.js -o dist/_/bundle.js && cp public/index.html dist",
    "build:public": "cp -r public/_ dist",
    "build:sw": "browserify -p tinyify -t [ babelify --presets [ \"@babel/preset-env\" ] ] -o dist/sw.js sw.js",
    "build": "npm run build:index && npm run build:sw && npm run build:public",
    "sw:watch": "watchify sw.js -t [ babelify --presets [ \"@babel/preset-env\" ] ] -o public/sw.js -v"
  },
  "author": "Jesse Kriss <jesse@jklabs.net>",
  "license": "ISC",
  "browserslist": "> 1%, not dead",
  "dependencies": {
    "instascan": "git+https://github.com/jkriss/instascan.git",
    "nanohtml": "^1.9.0",
    "powfile": "0.0.3",
    "qrcode": "^1.4.2",
    "qrloop": "^0.9.0",
    "zip-server": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "core-js": "^3.3.4",
    "regenerator-runtime": "^0.13.3",
    "tinyify": "^2.5.1",
    "watchify": "^3.11.1"
  }
}
